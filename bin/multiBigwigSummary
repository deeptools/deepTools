#!/usr/bin/env python
#-*- coding: utf-8 -*-

import deeptools.misc
from deeptools.multiBigwigSummary import main
import sys

if __name__ == "__main__":
    args = None
    if len(sys.argv) == 1:
        args = ["--help"]
    if len(sys.argv) == 2 and sys.argv[1] != "--version":
        sys.argv.append("--help")
    main(args)
